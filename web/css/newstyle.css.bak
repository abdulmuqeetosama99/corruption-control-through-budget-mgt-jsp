#menu {
	width: 995px;
	height: 60px;
	background:black;
	margin: 0 auto;
}

#menu ul {
	margin: 0;
	padding: 0;
	list-style: none;
}

#menu li {
	display: inline;
	text-transform: uppercase;
}

#menu a {
	display: block;
	float: left;
	
	padding: 18px 30px 0 15px;
	text-decoration: none;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 1em;
	font-weight: bold;
	color: #FFFFFF;
}
#menu a:hover { color: red; text-decoration:none}
#footer
{width: 995px;
	height: 60px;
	margin: 0 auto;
}